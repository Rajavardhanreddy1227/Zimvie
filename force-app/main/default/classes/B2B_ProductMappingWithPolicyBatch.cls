global class B2B_ProductMappingWithPolicyBatch implements Database.Batchable<SObject>, Schedulable {
    public Database.QueryLocator start(Database.BatchableContext BC) {
        Datetime oneHourAgo = System.now().addHours(-Integer.valueof(System.Label.B2B_ProductBatchHours));
        String query;
        if(system.label.productbatchOneTimeFlag == 'true'){
            query = 'SELECT Id, Product2Id,Pricebook2Id FROM PriceBookEntry order by Pricebook2Id';
        } else {
            query = 'SELECT Id, Product2Id,Pricebook2Id FROM PriceBookEntry WHERE CreatedDate >= :oneHourAgo AND IsActive = True order by Pricebook2Id';
        }
        return Database.getQueryLocator(query);
    }

    public void execute(Database.BatchableContext BC, List<PriceBookEntry> scope) {
        final DCM_Logger logger = DCM_Logger.getInstance(B2B_SetupConstants.PAYMENT_TERM);
        try
        {
            Map<Id,Set<Id>> mapPbIdToProdId = new Map<Id,Set<Id>>();
            
            for(PriceBookEntry objPbe : scope){
                if(!mapPbIdToProdId.containskey(objPbe.Pricebook2Id)){
                    mapPbIdToProdId.put(objPbe.Pricebook2Id,new set<Id>());
                }
                mapPbIdToProdId.get(objPbe.Pricebook2Id).add(objPbe.Product2Id);
            }
            system.debug('pricebookIds=='+mapPbIdToProdId.keyset());
            
            Map<Id,Id> MapPbIdToBgId = new Map<Id,Id>();
            for(BuyerGroupPricebook objBGP : [Select BuyerGroupId,Pricebook2Id from BuyerGroupPricebook where Pricebook2Id IN:mapPbIdToProdId.keyset()]){
                MapPbIdToBgId.put(objBGP.Pricebook2Id,objBGP.BuyerGroupId);
            }
            system.debug('MapPbIdToBgId=='+MapPbIdToBgId);
            Map<Id,Id> MapBgIdToPolicyId = new Map<Id,Id>();
            for(CommerceEntitlementBuyerGroup objCEBG : [Select PolicyId,BuyerGroupId from CommerceEntitlementBuyerGroup where BuyerGroupId IN:MapPbIdToBgId.values()]){
                MapBgIdToPolicyId.put(objCEBG.BuyerGroupId,objCEBG.PolicyId);
            }
            system.debug('MapBgIdToPolicyId=='+MapBgIdToPolicyId);

            if(system.label.productbatchOneTimeFlag != 'true'){
                List<CommerceEntitlementProduct> lstCEPtoDel = new List<CommerceEntitlementProduct>();
                lstCEPtoDel = [Select Id from CommerceEntitlementProduct where PolicyId IN:MapBgIdToPolicyId.values()];
                if(lstCEPtoDel != null && lstCEPtoDel.size() > 0){
                    delete lstCEPtoDel;
                }
            }
            Set<String> setUniqueValues = new Set<String>();
            Set<Id> uniqueProductId = new Set<Id>();
            List<CommerceEntitlementProduct> lstCEPToInsert = new List<CommerceEntitlementProduct>();


            for(Id pbId :mapPbIdToProdId.keyset()){
                for(Id prodId : mapPbIdToProdId.get(pbId)){
                    CommerceEntitlementProduct objCEP = new CommerceEntitlementProduct();
                    objCEP.ProductId = prodId;
                    objCEP.PolicyId = MapBgIdToPolicyId.get(MapPbIdToBgId.get(pbId));
                    lstCEPToInsert.add(objCEP);
                }
            }    

            database.insert(lstCEPToInsert,false);
            
        }   
        catch(exception e){
            logger.error(e);
        }
        finally{
            logger.flush();
        }
    }

    public void finish(Database.BatchableContext BC) {
        // Execute any post-processing logic here
        //codeCov();
    }

    // Schedulable Interface Method
    global void execute(SchedulableContext SC) {
        
        B2B_ProductMappingWithPolicyBatch batch = new B2B_ProductMappingWithPolicyBatch();
        Database.executeBatch(batch);
        
    }
    

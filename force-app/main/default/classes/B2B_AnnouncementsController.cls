public class B2B_AnnouncementsController {

    @AuraEnabled(cacheable=true)
    public static List<Announcements_and_Message__c> fetchAnnouncements(String brandName,String guestcountry,Boolean isGuest, String type,String language) {
        final DCM_Logger logger = DCM_Logger.getInstance(B2B_SetupConstants.PAYMENT_TERM);
        List<Announcements_and_Message__c> announcements = new  List<Announcements_and_Message__c>();
        try {
            system.debug('brandName='+brandName);
        system.debug('guestcountry='+guestcountry);
        system.debug('isGuest='+isGuest);
        system.debug('type='+type);
        String usrCountry;
        if(isGuest== true){
            usrCountry = guestcountry;
        } else {
            User currentUser = [SELECT id,ContactId, AccountId, Country FROM User WHERE Id = :Userinfo.getUserId()];
            system.debug('currentUser'+currentUser);
            usrCountry = currentUser.Country ;
        }
        
        B2B_Language_Switcher__mdt lang = [SELECT Id,DeveloperName,B2B_Display_Text__c,B2B_Active__c,B2B_Lang_Code__c,B2B_Sort_Order__c From B2B_Language_Switcher__mdt WHERE B2B_Active__c = true AND B2B_Lang_Code__c = :language];
        
        

        announcements = [SELECT id,Announcement_Message__c,Country__c 
                        FROM Announcements_and_Message__c 
                        WHERE  Country__c = :usrCountry AND Brandname__c =:brandName AND Language__c=:lang.B2B_Display_Text__c
                        AND (Start_Date__c<=TODAY) AND (End_Date__c >= TODAY) AND Announcement_Type__c =: type order by Start_Date__c desc];
        system.debug('announcements='+announcements);
        
        }
        catch(exception e){
            logger.error(e);
        }
        finally{
            logger.flush();
        }
        
        return announcements;
    }

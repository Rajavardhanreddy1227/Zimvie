<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
  <isExposed>true</isExposed>
  <masterLabel>Ortho Default Listview</masterLabel>
  <description>Automatically redirect to a listview</description>
  <targets>
      <target>lightning__AppPage</target>
  </targets>
  <targetConfigs>
    <targetConfig targets="lightning__AppPage">
        <property name="listView" label="Listview" type="String" datasource="apex://Ortho_ListviewSources" description="Listview to redirect to" ></property>
    </targetConfig>
  </targetConfigs>
</LightningComponentBundle>